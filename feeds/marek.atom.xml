&lt;/span&gt;&lt;a name="line-33"&gt;&lt;/a&gt;&lt;span class="lineno"&gt;33 &lt;/span&gt;        &lt;span class="bp"&gt;self&lt;/span&gt;&lt;span class="o"&gt;.&lt;/span&gt;&lt;span class="n"&gt;down&lt;/span&gt;&lt;span class="p"&gt;()&lt;/span&gt;
